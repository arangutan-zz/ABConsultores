<h1>Listing empresas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre comercial</th>
      <th>Razon social</th>
      <th>Nit</th>
      <th>Representante legal</th>
      <th>Nombre contacto</th>
      <th>Email contacto</th>
      <th>Telefono contacto</th>
      <th>Direccion contacto</th>
      <th>Ciudad</th>
      <th>Pais</th>
      <th>Pagina web</th>
      <th>Imagen</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.nombre_comercial %></td>
        <td><%= empresa.razon_social %></td>
        <td><%= empresa.nit %></td>
        <td><%= empresa.representante_legal %></td>
        <td><%= empresa.nombre_contacto %></td>
        <td><%= empresa.email_contacto %></td>
        <td><%= empresa.telefono_contacto %></td>
        <td><%= empresa.direccion_contacto %></td>
        <td><%= empresa.ciudad %></td>
        <td><%= empresa.pais %></td>
        <td><%= empresa.pagina_web %></td>
        <td><%= image_tag empresa.imagen_url , class: 'imagen' %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
