= form_for(@stakeholder, url: { action: 'influencia' })  do |f| 
	-if @stakeholder.errors.any? 
		#error_explanation
			%h2
				= pluralize(@stakeholder.errors.count, "error")
				prohibited this stakeholder from being saved:
		- @stakeholder.errors.full_messages.each do |message|		
			%li
				= message

	.row
		.large-4.columns
			= f.label :diposicion
			= f.text_field :disposicion
		.large-4.columns
			= f.label :expectativas
			= f.text_field :expectativas
		.large-4.columns
			= f.label :dimension
			= f.text_field :dimension

	.row
		.large-4.columns
			= f.label :dependencia
			= f.text_field :dependencia
		.large-4.columns
		.large-4.columns
			%p
				Historial de clasificaci√≥n
	.row
		.large-12.columns.text-center
			= f.submit 'Siguiente', class: 'button verde-bg'